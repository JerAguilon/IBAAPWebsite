        <div id = "cal-heatmap"></div>
        <script type = "text/javascript">

            var date = new Date();
            console.log(date.getFullYear());

            var startDate = date.getFullYear();

            if (date.getMonth() - 3 < 1) {
                startDate--;
            }

            var cal = new CalHeatMap();
            cal.init({
                subDomainTextFormat: "%d",
                range:4,
                domain: "month",
                data: "datas.json",
                cellSize:20,
                start: new Date(startDate, date.getMonth() -3, date.getDate())
            });
        </script>


